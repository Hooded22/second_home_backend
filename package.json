{
  "name": "second_home_backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev --respawn server.ts --experimental-vm-modules",
    "dev": "nodemon --exec ts-node server.ts",
    "test": "jest --watchAll",
    "test:coverage": "jest --watchAll --coverage",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prettier-check": "prettier --check --no-error-on-unmatched-pattern \"src/**/*.ts\"",
    "prettier-write": "prettier --write --no-error-on-unmatched-pattern \"src/**/*.ts\""
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jest": "^28.1.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^2.4.0",
    "mongoose": "^5.13.0",
    "nodemon": "^2.0.9",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.12",
    "@babel/plugin-transform-typescript": "^7.17.12",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^27.5.1",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^2.3.2",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.0.0",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.7",
    "typescript": "^4.3.5"
  }
}
